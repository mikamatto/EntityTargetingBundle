{
    "name": "mikamatto/entity-targeting-bundle",
    "description": "A Symfony bundle to manage and filter entities based on customizable targeting criteria, ideal for audience-specific content management.",
    "type": "symfony-bundle",
    "license": "MIT",
    "version": "1.0.0",
    "autoload": {
        "psr-4": { "Mikamatto\\EntityTargetingBundle\\": "src/" }
    },
    "require": {
        "php": "^8.1",
        "symfony/framework-bundle": "^6.1|^7.0",
        "symfony/dependency-injection": "^6.1|^7.0",
        "symfony/security-core": "^6.1|^7.0",
        "symfony/cache": "^6.1|^7.0",
        "doctrine/orm": "^3.2",
        "doctrine/doctrine-bundle": "^2.8"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "authors": [
        {
            "name": "Mikamatto",
            "email": "mick@mikamatto.com"
        }
    ],
    "support": {
        "issues": "https://github.com/your-github-account/entity-targeting-bundle/issues",
        "source": "https://github.com/your-github-account/entity-targeting-bundle"
    },
    "keywords": [
        "symfony",
        "bundle",
        "entity",
        "targeting",
        "criteria",
        "filtering",
        "audience",
        "user-roles",
        "role-filter"
    ]
}